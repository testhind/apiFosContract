{
  "Data": {
    "Cds": {
      "ProviderName": "Microsoft.Data.SqlClient",
      "APITemplate": {
        "Infrastructure": {
          "Data": {
            "ConnectionStringName": "DefaultConnection"
          }
        }
      }
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "Cds.APITemplate.Api": "Warning",
      "Microsoft.AspNetCore.Mvc.Razor": "Error",
      "Microsoft.AspNetCore.Mvc.Razor.Razor": "Warning",
      "Microsoft.AspNetCore.Mvc.Razor.Internal": "Warning"
    }
  },
  "ConnectionStrings": {
    // Here goes the connection string of your test database.
    // You can find more informations and the value of the connecion string in  the readme located into the db directory of your project.
    // For example it shoud look like :
    // "DefaultConnection": "Server=mssqldb,1433;Database=mymsdatabase;User ID=sa;Password=mymsDatabase-123;Trusted_Connection=False;MultipleActiveResultSets=true;"
    "DefaultConnection": "Server=[SERVER] ; Database=[Database] ; Integrated Security=SSPI ; MultipleActiveResultSets=True"
  },

  "AllowedHosts": "*"
}